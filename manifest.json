{
	"manifest_version": 2,

	"name": "Zyncco",
	"version": "1.0.0",
	"version_name": "Zync Arsenide",
	"author": "Zyncco <support@zync.co>",
	"homepage_url": "https://zync.co",

	"description": "End-to-end encrypted cloud clipboard synchronization.",
	"icons": {},

	// "incognito": "not_allowed",
	"minimum_chrome_version": "42",
	"offline_enabled": false,
	
	"key": "gonkkepdmdkopdeapomlcleddingnbgl",
	
	"background":
	{
		"page": "/background/index.html",
		"persistent": true
	},

	"browser_action":
	{
		"default_popup": "/popup/index.html",
		"default_title": "Zyncco"
	},

	"options_ui":
	{
		"chrome_style": true,
		"page": "/window/options.html"
	},

	"permissions": [
		"background",
		"contextMenus",
		"clipboardRead",
		"clipboardWrite",
		"storage"
	]
}
